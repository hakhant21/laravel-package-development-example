{
    "name": "hak/data-fetcher",
    "description": "Fetch data from third party api",
    "authors": [
        {
            "name": "Htet Aung Khant",
            "email": "hakhant21@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.6"
    },
    "require-dev": {
        "pestphp/pest": "^2.6",
        "orchestra/testbench": "^8.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hak\\DataFetcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hak\\DataFetcher\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hak\\DataFetcher\\FetchServiceProvider"                
            ]
        }
    },
    "license": "MIT"
}
